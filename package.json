{
  "name": "deezic",
  "version": "0.1.0",
  "description": "Desktop Music Player for Deezer",
  "main": "./app/main/index.js",
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testRegex": "test/.*|\\.test\\.js"
  },
  "scripts": {
    "watch": "webpack-dev-server --config webpack.development.config.js --progress",
    "build": "webpack --config webpack.production.config.js -p",
    "start-dev": "NODE_ENV=development electron .",
    "start": "electron .",
    "lint": "eslint app/**/*.js",
    "format": "prettier --single-quote --write \"app/**/*.js\"",
    "test": "jest"
  },
  "author": "AbsoluteZero273",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.3",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^20.0.4",
    "prettier": "^1.3.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "electron": "^1.6.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
